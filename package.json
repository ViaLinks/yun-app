  "scripts": {
    "build:androidDebug": "babel-node --config-file ./build/babel.config.js ./build/build:androidDebug.js",
    "android": "npm run build:androidDebug && adb install -r ./Android/app/build/outputs/apk/debug/app-debug.apk && adb shell am start -n com.yunapp.android/.MainActivity",
    "dev": "webpack-dev-server --config ./build/webpack.dev.config.js --progress",
    "buildService:dev": "webpack --config ./build/service.webpack.config.js --mode development",
    "buildPages:dev": "webpack --config ./build/pages.webpack.config.js --mode development",
    "build:dev": "npm run clean && babel-node --config-file ./build/babel.config.js ./build/build:dev.js",
    "clean": "rm -rf ./dist/"
  },
